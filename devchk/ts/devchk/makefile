ifdef TET_ROOT
LIBDIR = $(TET_ROOT)/lib/tet3
INCDIR = $(TET_ROOT)/inc/tet3
EXTRALIBS = $(LIBDIR)/tcm.o $(LIBDIR)/libapi.a
else
EXTRALIBS =
endif

LSB_VERSION_FLAG = -D__LSB_VERSION__=41
# There is no need in this flag since LSB 4.0
LDBL_FLAG = 

# File with CFLAGS and CXXFLAGS
include cflags.mk

all: hdrchk

hdrchk: $(OBJS)
	$(CXX) -o hdrchk $(OBJS) $(EXTRALIBS) -L/usr/X11R6/lib64 -L/usr/X11R6/lib -L/usr/lib/qt3/lib64 -L/usr/lib/qt3/lib -L/usr/lib64/qt-3.3/lib -L/usr/lib/qt-3.3/lib -L/opt/gnome/lib64 -L/opt/gnome/lib -lXt -lX11 -lncurses -lqt-mt -lrt -lxml2 -lglib-2.0 -lgdk-x11-2.0 -lgtk-x11-2.0

hdrchk.o: hdrchk.c
	$(CC) $(CFLAGS) -g -c hdrchk.c


hdrchk.c:
	./mktests -v `cat LSB_VERSION`


gensrc: hdrchk.c


install:


clean:
	rm -f $(OBJS) hdrchk

clobber:	clean
	rm -f *.c hdrchk_tet.h testdefs.h


distclean:	clobber
